.QMenuBar{
padding:0px;
margin:0px;
border:0px;
font-weight:bold;}
.QMenuBar::item,
.QMenuBar::item:selected {
font-weight:bold;
border:0px;
padding:2px 3px;
padding-bottom:6px;
color:palette(highlight-text);}
.QMenuBar::item:disabled {
color:#656666;}
.QMenuBar::item:pressed {
margin-bottom:2px;
color:palette(highlight);
background:rgba(0,0,0,0.4);}
